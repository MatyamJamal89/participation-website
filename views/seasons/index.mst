<!DOCTYPE html>
<html>

{{> partials/head }}

<body>
  <div class="container is-fluid">
    <div class="columns is-gapless is-multiline">
      <div class="column is-full">
        {{> partials/header }}
      </div>
      <div class="column is-1">
        {{> partials/nav }}
      </div>
      <div class="column content">
        <div class="navbar-menu sub-navbar-menu">
          <div class="navbar-start">
            <h2 class="title">
              {{title}}
            </h2>
          </div>

          <div class="navbar-end">
            <a class="button is-link" href="/associations/{{associationId}}">
              العودة للرابطة
            </a>
          </div>
        </div>
        <div class="sub-content">
          <table>
            <th>اسم الموسم</th>
            {{#seasons}}
              <tr>
                <td>
                  <a href="/seasons/{{id}}">
                    {{name}}
                  </a>
                </td>
              </tr>
            {{/seasons}}
          </table>
        </div>
      </div>
    </div>
  </div>
</body>
</html>
